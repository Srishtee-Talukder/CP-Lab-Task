#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, x, sum = 0, odd = 0, even = 0;
        cin >> n;
        for (int i = 0; i < n; i++) {
            cin >> x;
            sum += x;
            if (x % 2) odd++;
            else even++;
        }
        if (sum % 2 == 1) cout << "YES\n";
        else if (odd > 0 && even > 0) cout << "YES\n";
        else cout << "NO\n";
    }
    return 0;
}
