#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, ans = 0;
        cin >> n;
        for (int d = 1; d <= 9; d++) {
            int x = d;
            while (x <= n) {
                ans++;
                x = x * 10 + d;
            }
        }
        cout << ans << "\n";
    }
    return 0;
}
