#include <bits/stdc++.h>
using namespace std;

int main() {
    int t; cin >> t;
    while(t--) {
        string s; cin >> s;
        if((s.back()-'0')%2==0) cout<<0<<"\n";
        else if((s[0]-'0')%2==0) cout<<1<<"\n";
        else {
            int f=0;
            for(char c:s) if((c-'0')%2==0) f=1;
            cout<<(f?2:-1)<<"\n";
        }
    }
}
