#include <bits/stdc++.h>
using namespace std;

int main() {
    int t; cin >> t;
    while(t--) {
        int n; cin >> n;
        vector<int> a(n), freq(n+1,0);
        for(int &x:a) { cin >> x; freq[x]++; }
        int mx = *max_element(freq.begin(), freq.end());
        cout << n-mx << "\n";
    }
}
