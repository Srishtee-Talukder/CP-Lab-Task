#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> a(n);
    for (int &x : a) cin >> x;

    vector<int> b = a;
    sort(b.begin(), b.end());

    int l = 0, r = n - 1;
    while (l < n && a[l] == b[l]) l++;
    while (r >= 0 && a[r] == b[r]) r--;

    if (l > r) {
        cout << "yes\n1 1\n";
        return 0;
    }

    reverse(a.begin() + l, a.begin() + r + 1);

    if (a == b)
        cout << "yes\n" << l + 1 << " " << r + 1 << "\n";
    else
        cout << "no\n";

    return 0;
}
