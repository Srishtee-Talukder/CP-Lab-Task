#include <bits/stdc++.h>
using namespace std;

vector<int> dijkstra(int n, vector<vector<int>> adj[], int src) {
    vector<int> dist(n, 1e9);
    vector<int> vis(n, 0);
    dist[src] = 0;

    for (int i = 0; i < n; i++) {
        int u = -1;
        for (int j = 0; j < n; j++)
            if (!vis[j] && (u == -1 || dist[j] < dist[u]))
                u = j;

        vis[u] = 1;

        for (auto &p : adj[u]) {
            int v = p[0], w = p[1];
            if (dist[u] + w < dist[v])
                dist[v] = dist[u] + w;
        }
    }
    return dist;
}

int main() {
    int n;
    cin >> n;
    vector<vector<int>> adj[n];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++) {
            int x;
            cin >> x;
            if (x > 0) adj[i].push_back({j, x});
        }

    int src;
    cin >> src;

    vector<int> ans = dijkstra(n, adj, src);

    for (int x : ans) cout << x << " ";
}
