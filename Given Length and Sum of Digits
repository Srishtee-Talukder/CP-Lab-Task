#include <bits/stdc++.h>
using namespace std;

int main() {
    int m, s;
    cin >> m >> s;
    if ((s == 0 && m > 1) || s > 9 * m) { cout << "-1 -1\n"; return 0; }
    if (s == 0) { cout << "0 0\n"; return 0; }

    int sum = s;
    string maxNum, minNum;
    for (int i = 0; i < m; i++) {
        int d = min(9, sum);
        maxNum += '0' + d;
        sum -= d;
    }

    sum = s;
    for (int i = 0; i < m; i++)
        for (int d = (i == 0 ? 1 : 0); d <= 9; d++)
            if (sum - d <= 9 * (m - i - 1)) { minNum += '0' + d; sum -= d; break; }

    cout << minNum << " " << maxNum << "\n";
    return 0;
}
