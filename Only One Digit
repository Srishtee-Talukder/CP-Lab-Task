#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int x;
        cin >> x;
        string a = to_string(x);

        for (int y = 0; ; y++) {
            string b = to_string(y);
            bool match = false;
            for (char c : a)
                for (char d : b)
                    if (c == d) match = true;

            if (match) {
                cout << y << "\n";
                break;
            }
        }
    }
    return 0;
}
