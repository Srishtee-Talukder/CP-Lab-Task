#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int a1, a2, a4, a5;
        cin >> a1 >> a2 >> a4 >> a5;
        int ans = 0;
        for (int a3 : {a1 + a2, a4 - a2, a5 - a4}) {
            int f = 0;
            if (a3 == a1 + a2) f++;
            if (a4 == a2 + a3) f++;
            if (a5 == a3 + a4) f++;
            ans = max(ans, f);
        }
        cout << ans << "\n";
    }
}
