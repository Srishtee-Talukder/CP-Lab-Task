#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int odd = 0, even = 0, x;
        for (int i = 0; i < 2*n; i++) {
            cin >> x;
            if (x % 2) odd++;
            else even++;
        }
        cout << ((odd == even) ? "Yes\n" : "No\n");
    }
    return 0;
}
