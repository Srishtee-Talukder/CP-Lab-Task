#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        string s;
        cin >> s;
        string t = "abc";
        if (s == t) {
            cout << "YES\n";
            continue;
        }
        bool ok = false;
        for (int i = 0; i < 3; i++) {
            for (int j = i + 1; j < 3; j++) {
                swap(s[i], s[j]);
                if (s == t) ok = true;
                swap(s[i], s[j]);
            }
        }
        cout << (ok ? "YES\n" : "NO\n");
    }
    return 0;
}
