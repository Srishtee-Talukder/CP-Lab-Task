#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        int x, cur = 0, ans = 0;
        for (int i = 0; i < n; i++) {
            cin >> x;
            if (x == 0) cur++;
            else cur = 0;
            ans = max(ans, cur);
        }
        cout << ans << "\n";
    }
    return 0;
}
