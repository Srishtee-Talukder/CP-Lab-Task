#include <iostream>
using namespace std;

int main() {
    int k2,k3,k5,k6; 
    cin >> k2 >> k3 >> k5 >> k6;
    
    int c256 = k2;
    if(k5 < c256) c256 = k5;
    if(k6 < c256) c256 = k6;
    
    k2 -= c256;
    
    int c32 = k2;
    if(k3 < c32) c32 = k3;
    
    cout << c256*256 + c32*32 << "\n";
}
