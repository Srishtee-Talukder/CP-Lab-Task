#include <bits/stdc++.h>
using namespace std;

int main() {
    int q; cin >> q;
    while(q--) {
        int n; string t; cin >> n >> t;
        string s;
        for(int i=n-1;i>=0;) {
            if(t[i]=='0') s += 'a'+(t[i-2]-'0')*10 + (t[i-1]-'0') -1, i-=3;
            else s += 'a'+(t[i]-'0')-1, i--;
        }
        reverse(s.begin(), s.end());
        cout << s << "\n";
    }
}
