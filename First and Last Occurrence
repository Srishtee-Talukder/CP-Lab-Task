#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    int arr[n];
    for(int i = 0; i < n; i++)
        cin >> arr[i];

    int X;
    cin >> X;

    int first = -1, last = -1;

    for(int i = 0; i < n; i++) {
        if(arr[i] == X) {
            if(first == -1)
                first = i;
            last = i;
        }
    }

    cout << first << " " << last;
    return 0;
}
