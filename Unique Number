#include <bits/stdc++.h>
using namespace std;

int main() {
    int t; cin >> t;
    while (t--) {
        int x; cin >> x;
        if (x > 45) { cout << -1 << "\n"; continue; }
        string ans = "";
        for (int d = 9; d >= 1 && x > 0; d--) {
            if (x >= d) { ans = char('0'+d) + ans; x -= d; }
        }
        cout << ans << "\n";
    }
}
